{% extends 'admin/base_list.html.twig' %}
{% block list_title %}
    Usuarios Directores
{% endblock %}

{% block boton_nuevo %}
    <a href="{{ path('nuevo miembro') }}"><button class="btn btn-info" type="button" style="margin:15px;"><i class="fa fa-plus"></i> Nuevo Director</button></a>
{% endblock %}    

{% block list_header %}
    <th>Nombre</th>
    <th>Director</th>
    <th>Colegio</th>
    <th>Email</th>
    <th>Operaciones</th>
    {% endblock %}   

{% block list_body %}
    {% for miembro in miembros %} 
        {{ include('admin/miembros/list_row.html.twig', { 'miembro': miembro }) }} 
    {% endfor %} 
{% endblock %}
{% block list_footer %}
    <th>Nombre</th>
    <th>Director</th>
    <th>Colegio</th>
    <th>Email</th>
    <th>Operaciones</th>
    {% endblock %}

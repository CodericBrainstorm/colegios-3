<tr class="gradeX">
    <td>{{ sostenedor.username }}</td>
    <td>{{ sostenedor.apellido }} {{ sostenedor.nombre }}</td>
    <td>{{ (sostenedor.tipoInstitucion is null ? ('views.sin_asignar' | trans) : sostenedor.tipoInstitucion.nombre) }}</td>
    <td>{{ sostenedor.email }}</td>
    <td>{{ sostenedor.direccion }}</td>
    <td>{{ (sostenedor.comuna is null ? ('views.sin_asignar' | trans) : sostenedor.comuna.nombre) }}</td>
    <td>{{ (sostenedor.comuna is null ? ('views.sin_asignar' | trans) : sostenedor.comuna.ciudad.nombre) }}</td>
    <td class="center">
        <div class="btn-group">
            <button data-toggle="dropdown" class="btn btn-info dropdown-toggle" type="button">Operaciones <span class="caret"></span></button>
            <ul role="menu" class="dropdown-menu">
                <li><a href="{{ path('ver sostenedor', {'id': sostenedor.id}) }}">Ver</a></li>
                <li><a href="{{ path('editar sostenedor', {'id': sostenedor.id}) }}">Editar</a></li>
                <li><a href="#sostenedorModal{{sostenedor.id}}" data-toggle="modal">Eliminar</a></li>
                <li class="divider"></li>
                <li><a href="{{ path('ver directores sostenedor admin', {'id': sostenedor.id}) }}">Ver Directores</a></li>
                <li><a href="{{ path('ver compromisos sostenedor', {'id': sostenedor.id}) }}">Ver Compromisos</a></li>
                <li><a href="{{ path('ver compromisos asignados sostenedor', {'idSostenedor': sostenedor.id}) }}">Ver Compromisos Asginados</a></li>
            </ul>
        </div>
    </td>
</tr>
<div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="sostenedorModal{{sostenedor.id}}" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button aria-hidden="true" data-dismiss="modal" class="close" type="button">Ã—</button>
                <h4 class="modal-title">{{ 'sostenedor.views.delete.title'|trans }}</h4>
            </div>
            <div class="modal-body">

                {{ 'sostenedor.views.delete.message'|trans|nl2br }}

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">{{ 'sostenedor.views.delete.cancel'|trans }}</button>
                <a href="{{ path('eliminar sostenedor', {'id': sostenedor.id}) }}"><button type="button" class="btn btn-danger"> {{ 'sostenedor.views.delete.confirm'|trans }}</button></a>
            </div>
        </div>
    </div>
</div>

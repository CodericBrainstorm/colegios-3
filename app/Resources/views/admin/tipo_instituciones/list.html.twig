{% endblock %}    

{% block list_header %}
    <th>Nombre</th>
    <th>Operaciones</th>
    {% endblock %}   

{% block list_body %}
    {% for tipo_institucion in tipo_instituciones %} 
        {{ include('admin/tipo_instituciones/list_row.html.twig', { 'tipo_institucion': tipo_institucion }) }} 
    {% endfor %} 
{% endblock %}
{% block list_footer %}
    <th>Nombre</th>
    <th>Operaciones</th>
{% endblock %}
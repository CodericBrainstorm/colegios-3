{% extends 'base_list.html.twig' %}
{% block list_title %}
    Compromisos {% if sostenedor is defined %} del Sostenedor: <span style="color:#5bc0de;">{{sostenedor.nombre~" "~sostenedor.apellido}}</span> {% endif %}
{% endblock %}

{% block list_header %}
    <th>Nombre</th>
    <th>Área</th>
    <th>Sostenedor</th>
    <th>Operaciones</th>
    {% endblock %}   

{% block list_body %}
    {% for compromiso in compromisos %} 
        {{ include('sostenedor/compromisos/list_row.html.twig', { 'compromiso': compromiso }) }} 
    {% endfor %} 
{% endblock %}
{% block list_footer %}
    <th>Nombre</th>
    <th>Área</th>
    <th>Sostenedor</th>
    <th>Operaciones</th>
    {% endblock %}
